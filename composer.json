{
  "name": "codeinc/router-routable-resolver",
  "version": "1.1.1",
  "description": "A Code Inc.'s router resolver using interfaces to detect and configure request handlers",
  "homepage": "https://github.com/CodeIncHQ/RouterRoutableResolver",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "codeinc/router": "^4",
    "codeinc/directory-classes-iterator": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "codeinc/error-renderer": "^1.2",
    "codeinc/router": "4.x@dev"
  },
  "autoload": {
    "psr-4": {"CodeInc\\RouterRoutableResolver\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\RouterRoutableResolver\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "repositories": [
    { "type": "vcs", "url": "https://github.com/CodeIncHQ/Router" }
  ]
}
